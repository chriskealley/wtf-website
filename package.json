{
  "name": "WFT Astro website template",
  "type": "module",
  "version": "0.0.1",
  "license": "MIT",
  "description": "A website template for the WFT collection",
  "author": "Chris Kealley",
  "repository": {
    "type": "git",
    "url": "https://github.com/chriskealley/wtf-website"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
